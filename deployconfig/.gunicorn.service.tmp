[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=root
Group=nginx
WorkingDirectory=/home/www/ProjectName-0001/mysite
ExecStart=/home/www/ProjectName-0001/env/bin/gunicorn --workers 3 --bind unix:/home/www/ProjectName-0001/mysite/mysite.sock mysite.wsgi:application

[Install]
WantedBy=multi-user.target